  "scripts": {
    "lint": "eslint --format node_modules/eslint-friendly-formatter --ext .jsx --ext .js src && stylelint ./src/**/*.scss",
    "start": "NODE_ENV=dev webpack-dev-server --hot --config ./webpack/webpack.local.conf",
    "dev": "NODE_ENV=dev webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "cqa": "NODE_ENV=cqa webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "prod": "NODE_ENV=prod webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "build": "rimraf dist && npm run dev && npm run cqa && npm run prod",
    "test": "karma start ./webpack/karma/karma.conf",
    "prepush": "npm run lint && npm run test"
