  "main": "index.js",
  "scripts": {
    "lint": "eslint --format node_modules/eslint-friendly-formatter --ext .jsx --ext .js src && stylelint ./src/**/*.scss",
    "start": "NODE_ENV=development webpack-dev-server --hot --config ./webpack/webpack.local.conf",
    "dev": "NODE_ENV=dev webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "qa": "NODE_ENV=qa webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "production": "NODE_ENV=production webpack --config ./webpack/webpack.conf --progress --profile --bail",
    "build": "rimraf dist && npm run dev && npm run qa && npm run production",
    "test": "karma start ./webpack/karma/karma.conf",
    "prepush": "npm run lint && npm run test"
  },
